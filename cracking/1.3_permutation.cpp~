#include<iostream>
#include<stdlib.h>
#include<string.h>
using namespace std;

bool permutation(char* s1,char* s2){
int count=0;
while(*(s1+count) or *(s2+count)){
if(*(s1+count)==*(s2+count)){
count++;
}
else{
cout<<"not permutation\n";
exit(0);
}
}
cout<<"\n";
}

int main(){
cout<<"Enter a string\n";
char s1[30],s2[30];
cin>>s1;
cout<<"Enter another string\n";
cin>>s2;
permutation(s1,s2);
}


